<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="User">

	<!-- 로그인상태 조회 -->
	<select id="retrieveUser" resultType="com.olTeam.mibuckit.test.dto.TestPostManRes" parameterType="com.olTeam.mibuckit.test.dto.TestPostManReq">
		select ID, NAME, TLNO 
		from TB_MBK_USER_M
		where ID=1
	</select>
</mapper>